@Article{Valenzuela2014,
  Title                    = {{Optical and thermal performance of large-size parabolic-trough solar collectors from outdoor experiments: A test method and a case study}},
  Author                   = {Valenzuela, Loreto and L{\'{o}}pez-Mart{\'{i}}n, Rafael and Zarza, Eduardo},
  Journal                  = {Energy},
