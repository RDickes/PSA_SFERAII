@Article{Ohman_2012,
  Title                    = {Implementation and evaluation of a low temperature waste heat recovery power cycle using {NH3} in an organic {R}ankine cycle},
  Author                   = {Henrik {\"O}hman},
  Journal                  = {Energy},
  Year                     = {2012},
  Number                   = {1},
  Pages                    = {227 - 232},
  Volume                   = {48},

  Abstract                 = {With increasing cost for power generation opportunities for small scale power generation from waste heat have increased. The awareness of untapped resources such as local waste heat streams as well as the available range of technology and products to harvest such streams is increasing steadily though field data is scarce for applications below 100Â Â°C entry temperature. \{ORC\} applications have a large number of open parameters and therefore require field data for correlation of models. This paper presents field data and analysis of an \{ORC\} power generation plant operating with NH3. The unit operates on waste heat from a Swedish pulp mill at an available temperature of 75 to 85Â Â°C. Performance at varying heat source conditions and capacity is reported as well as an analysis of the particular investment case. The data was generated during a 15 day period and show a thermal efficiency of 8â€“9% at capacities from 50 to 100%. The results indicate a flat thermal efficiency from 20 to 100% capacity. Investment case analysis is based on a purchase model while the chosen economic model is a supplier own-and-operate arrangement supplying the mill with power at a predefined cost during an extended period of time.},
  Doi                      = {http://dx.doi.org/10.1016/j.energy.2012.02.074},
  ISSN                     = {0360-5442},
  Keywords                 = {\{ORC\}},
  Owner                    = {user},
  Timestamp                = {2014.10.17}
}

@Article{Ohman_2012_LysholmTurbine,
  Title                    = {Experimental investigation of a Lysholm Turbine operating with superheated, saturated and 2-phase inlet conditions },
  Author                   = {Henrik {\"O}hman and Per Lundqvist},
  Journal                  = {Applied Thermal Engineering },
  Year                     = {2012},
  Number                   = {1},
  Pages                    = {1211 - 1218},
  Volume                   = {50},

  Abstract                 = {Low temperature power cycles can benefit from the use of multi-phase flow expansion devices from a thermodynamic cycle efficiency point of view. Particularly power cycles such as ORC, Kalina and Trilateral Flash Cycles can be improved by multi-phase expansion. This article presents the experimental findings in a series of laboratory tests on a semihermetic Lysholm Turbine operating with \{R134a\} with superheated, saturated and wet inlet gas conditions. The test arrangements are described as well as discussion on the relevance of such test data. Finally comparison is made with findings from other investigations and recommendations for further studies are made. A correlation between peak efficiency and sensitivity to inlet vapour fraction was discovered which allows for estimations of adiabatic efficiencies with 2-phase inlet conditions even when only test data, or simulations, from single phase inlet conditions exist. The conclusions made are that Lysholm Turbines are well suited for low temperature power generation and that further understanding of the performance during 2-phase conditions is required.},
  Doi                      = {http://dx.doi.org/10.1016/j.applthermaleng.2012.08.035},
  ISSN                     = {1359-4311},
  Keywords                 = {2-phase},
  Owner                    = {user},
  Timestamp                = {2014.12.15}
}

